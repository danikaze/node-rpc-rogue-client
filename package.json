{
  "name": "node-rpc-rogue-client",
  "version": "0.1.0",
  "description": "Bootsrap for the Rogue client in node-rpc",
  "author": {
    "name": "danikaze",
    "email": "danikaze@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danikaze/node-rpc-rogue-client"
  },
  "license": "MIT",
  "engines": {
    "node": "10.15.x",
    "npm": "6.4.x"
  },
  "scripts": {
    "test": "npm run lint",
    "build": "webpack --config webpack.config.js --env=production",
    "dev": "webpack --config webpack.config.js --env=development",
    "lint": "tslint -p tsconfig.json -c tslint.yaml",
    "clean": "rimraf app",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier",
      "pre-push": "npm run test"
    }
  },
  "devDependencies": {
    "@types/mkdirp": "^0.5.2",
    "clean-webpack-plugin": "^3.0.0",
    "husky": "^4.0.7",
    "mkdirp": "^0.5.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-loader": "^6.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.20.1",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-lines-between-class-members": "^1.3.6",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {}
}
